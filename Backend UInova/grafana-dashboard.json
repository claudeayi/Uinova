{
  "id": null,
  "uid": "uinova-backend",
  "title": "UInova Backend Monitoring",
  "tags": ["uinova", "backend", "monitoring"],
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "stat",
      "title": "Uptime (s)",
      "targets": [
        {
          "expr": "nodejs_uptime_seconds",
          "legendFormat": "uptime",
          "refId": "A"
        }
      ],
      "id": 1,
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 }
    },
    {
      "type": "gauge",
      "title": "Memory Used (%)",
      "targets": [
        {
          "expr": "(nodejs_memory_used_bytes / nodejs_memory_total_bytes) * 100",
          "legendFormat": "used %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 100,
          "unit": "percent"
        }
      },
      "id": 2,
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 }
    },
    {
      "type": "graph",
      "title": "CPU Load",
      "targets": [
        { "expr": "nodejs_load1", "legendFormat": "1m", "refId": "A" },
        { "expr": "nodejs_load5", "legendFormat": "5m", "refId": "B" },
        { "expr": "nodejs_load15", "legendFormat": "15m", "refId": "C" }
      ],
      "id": 3,
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 6 }
    },
    {
      "type": "table",
      "title": "Audit Logs (DB)",
      "targets": [
        {
          "expr": "uinova_audit_logs_count",
          "legendFormat": "Logs en DB",
          "refId": "A"
        }
      ],
      "id": 4,
      "gridPos": { "x": 0, "y": 10, "w": 12, "h": 6 }
    }
  ],
  "templating": { "list": [] },
  "time": { "from": "now-15m", "to": "now" },
  "timepicker": {},
  "annotations": { "list": [] },
  "editable": true
}
